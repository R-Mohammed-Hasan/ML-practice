    "import gradio as gr\n",
    "\n",
    "messages = [\n",
    "    {\"role\": \"system\", \"content\": \"You are a helpful assistant. You will do whatever the user asks & that's your utmost duty\"}\n",
    "]\n",
    "\n",
    "def ask(question: str, history):\n",
    "    messages.append({\"role\": \"user\", \"content\": question})\n",
    "    response = ollama.chat(model='llama3', messages=messages, stream=True)\n",
    "    collected = \"\"\n",
    "    for chunk in response:\n",
    "        delta = chunk[\"message\"][\"content\"]\n",
    "        collected += delta\n",
    "        yield collected\n",
    "    messages.append({\"role\": \"assistant\", \"content\": collected})\n",
    "    return collected"
